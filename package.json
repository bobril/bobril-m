{
    "name": "bobril-m",
    "version": "0.16.0",
    "dependencies": {
        "bobril": "*",
        "bobril-m-icons": "*"
    },
    "main": "index.js",
    "typescript": {
        "main": "index.ts"
    },
    "prettier": {
        "tabWidth": 4
    },
    "bobril": {
        "title": "Bobril Material",
        "example": "example.ts",
        "compilerOptions": {
            "noImplicitAny": true,
            "noImplicitThis": true,
            "noUnusedLocals": true,
            "noUnusedParameters": true,
            "noImplicitReturns": true,
            "noFallthroughCasesInSwitch": true,
            "strictNullChecks": true,
            "strict": true
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bobril/bobril-m.git"
    },
    "keywords": [
        "bobril",
        "material"
    ],
    "author": "Boris Letocha",
    "license": "MIT",
    "devDependencies": {
        "typescript": "*"
    }
}